{
  "name": "@devoinc/browser-sdk",
  "version": "3.0.0",
  "description": "Devo browser SDK",
  "author": "Devo Dev Team",
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    }
  },
  "main": "index.js",
  "files": [
    "index.js",
    "lib/"
  ],
  "scripts": {
    "test": "nyc --reporter=lcov npm run mocha",
    "mocha": "mocha --ui bdd --require should --reporter mocha-sonarqube-reporter --reporter-options output=test-report.xml 'test/**/*.test.js'",
    "test:debug": "mocha --inspect-brk --reporter spec --timeout 20000 test/*.js",
    "posttest": "npm run lint",
    "lint": "eslint 'lib/**/*.js'",
    "release:prerelease": "npm version prerelease",
    "release:minor": "npm version minor",
    "release:major": "npm version major",
    "release:patch": "npm version patch",
    "release:publish": "npm publish",
    "release:preminor": "npm version preminor --preid=alpha",
    "manualtest": "mocha --reporter list --timeout 10000 test/manual/*.js",
    "postinstall": "mkdir -p dist",
    "webpack:prod": "webpack --config ./webpack/webpack.prod.js",
    "webpack:dev": "webpack-dev-server --watch-content-base -d --progress --config ./webpack/webpack.dev.js",
    "dist": "npm run webpack:prod"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:devoinc/browser-sdk.git"
  },
  "keywords": [
    "Devo",
    "SDK",
    "API",
    "Query",
    "LinQ"
  ],
  "dependencies": {
    "@devoinc/js-helper": "2.0.0",
    "abort-controller": "^3.0.0",
    "detect-browser": "^4.5.1",
    "fetch-readablestream": "^0.2.0",
    "node-fetch": "2.6.7",
    "web-streams-polyfill": "^2.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-transform-modules-commonjs": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.4.0",
    "@babel/runtime": "^7.4.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "css-loader": "^3.0.0",
    "eslint": "^6.0.1",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest-sonar-reporter": "^2.0.0",
    "mocha": "^6.1.4",
    "mocha-each": "^2.0.1",
    "mocha-jsdom": "^2.0.0",
    "mocha-sonarqube-reporter": "^1.0.2",
    "msw": "^0.22.3",
    "nyc": "^15.1.0",
    "regenerator-runtime": "^0.13.2",
    "sass-loader": "^7.0.1",
    "should": "^13.2.3",
    "sinon": "^9.2.1",
    "sonar-scanner": "^3.1.0",
    "style-loader": "^0.23.1",
    "uglify-es": "3.3.9",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.2"
  },
  "license": "MIT",
  "engines": {
    "node": ">=14",
    "npm": ">=7"
  }
}
